{
  "name": "mcb-integration-layer",
  "version": "1.0.0",
  "description": "Mcb Integration Layer",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "mocha --require ts-node/register --file tests/index.test.ts",
    "start": "ts-node src/app.ts",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@bitbucket.org/calmisland/mcb-integration-layer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/calmisland/mcb-integration-layer/issues"
  },
  "homepage": "https://bitbucket.org/calmisland/mcb-integration-layer#readme",
  "dependencies": {
    "@prisma/client": "3.5.0",
    "dotenv": "10.0.0",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@commitlint/cz-commitlint": "15.0.0",
    "@types/chai": "4.2.22",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.1",
    "@types/mocha": "9.0.0",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.8",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "4.3.4",
    "commitizen": "4.2.4",
    "eslint": "8.2.0",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "inquirer": "8.2.0",
    "mocha": "9.1.3",
    "morgan": "1.10.0",
    "nodemon": "2.0.15",
    "prettier": "2.4.1",
    "prisma": "3.5.0",
    "standard-version": "9.3.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
